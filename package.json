{
  "name": "@skymonster97/eslint-config",
  "version": "0.0.2-dev",
  "private": true,
  "description": "ESLint configuration package",
  "license": "UNLICENSED",
  "author": "Skymonster97",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint . --ext .js,.json --ignore-pattern test/",
    "lint:fix": "npx eslint . --ext .js,.json --fix --ignore-pattern test/",
    "test": "npx eslint test/ --ext .html,.js,.mjs,.json,.md",
    "test:rules": "npx eslint-find-rules -u index.js",
    "update": "npx ncu -u",
    "update:check": "npx ncu"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-find-rules": "^3.6.1",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.4",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^22.0.0",
    "npm-check-updates": "^9.0.4"
  },
  "peerDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0"
  },
  "optionalDependencies": {
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.4",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^22.0.0"
  },
  "engines": {
    "node": ">=10.4.0"
  }
}
